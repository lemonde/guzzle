{
    "name": "lmd/guzzle/plugin",
    "description": "Guzzle plugin component containing all Guzzle HTTP plugins",
    "homepage": "http://guzzlephp.org/",
    "keywords": ["http", "client", "plugin", "extension", "guzzle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dowling",
            "email": "mtdowling@gmail.com",
            "homepage": "https://github.com/mtdowling"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "lmd/guzzle/http": "self.version"
    },
    "suggest": {
        "lmd/guzzle/cache": "self.version",
        "lmd/guzzle/log": "self.version"
    },
    "autoload": {
        "psr-0": { "Lmd\\Guzzle\\Plugin": "" }
    },
    "target-dir": "Guzzle/Plugin",
    "replace": {
        "lmd/guzzle/plugin-async": "self.version",
        "lmd/guzzle/plugin-backoff": "self.version",
        "lmd/guzzle/plugin-cache": "self.version",
        "lmd/guzzle/plugin-cookie": "self.version",
        "lmd/guzzle/plugin-curlauth": "self.version",
        "lmd/guzzle/plugin-error-response": "self.version",
        "lmd/guzzle/plugin-history": "self.version",
        "lmd/guzzle/plugin-log": "self.version",
        "lmd/guzzle/plugin-md5": "self.version",
        "lmd/guzzle/plugin-mock": "self.version",
        "lmd/guzzle/plugin-oauth": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.7-dev"
        }
    }
}
